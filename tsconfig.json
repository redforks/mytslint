{
  "compilerOptions": {
    "alwaysStrict": true,
    "target": "es2017",
    "module": "commonjs",
    "declaration": true,
    "strict": true,
    "strictFunctionTypes": true,
    "strictNullChecks": true,
    "strictPropertyInitialization": true,
    "downlevelIteration": true,
    "lib": [
      "es2016",
      "dom"
    ],
    "importHelpers": true,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "removeComments": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true
  }
}
